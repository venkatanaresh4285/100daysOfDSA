Linkedlist Introduction

#include<bits/stdc++.h>
using namespace std;
struct Node{
  int data;
  Node* next;
  Node(int data1,Node* next1){
    data=data1;
    next=next1;
  }
   Node(int data1){
    data=data1;
    next=nullptr;
  }
};
int main(){
  vector<int> arr={2,3,4,5,6};
  Node* y= new Node(arr[3]);
  cout<<y->data;
}
